				api.get.unread_inbox_emails = function() {
					var dom = $("div[role=navigation]").find("[title*='" + api.tools.i18n('inbox') + "']");

					if(dom.length > 0) {
						if(dom[0].text.indexOf('(') != -1) {
							return parseInt(dom[0].text.replace(/[^0-9]/g, ''));
						}
					}

					return 0;
				};


				api.get.unread_draft_emails = function() {
